{
    "name": "contactlab/contacthub-sdk-php",
    "description": "Php SDK for ContactHub",
    "keywords": ["php","contactlab", "contacthub"],
    "type": "library",
    "license": "Apache-2.0",
    "require": {
        "php": ">=5.5",
        "guzzlehttp/guzzle": "^6.2",
        "ramsey/uuid": "^3.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.6",
        "symfony/var-dumper": "^3.2",
        "phpdocumentor/phpdocumentor": "^2",
        "cvuorinen/phpdoc-markdown-public": "^0.1"
    },
    "autoload": {
        "psr-4": {
            "ContactHub\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ContactHub\\Tests\\": "tests/"
        },
        "files": [
            "vendor/phpunit/phpunit/src/Framework/Assert/Functions.php"
        ]
    }
}
